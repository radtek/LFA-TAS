    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <!--<id name="CodeInt" column="CodeInt" type="int">
      <generator class="native" />
    </id>-->

    <property name="DealerId" />
    <property name="DealerLocation" />
    <property name="CountryId" />
    <property name="CityId" />
    <property name="Code" />


    <!--<generator class="native" />-->
    <property name="PurcheasedDate" />
    <property name="PlateNumber" />
    <property name="TireQuantity" />
    <property name="GeneratedDate" />
    <property name="GeneratedBy" />
