<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="TAS.Services" namespace="TAS.Services.Entities">
  <class name="InvoiceCodeDetails" table="InvoiceCodeDetails" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="InvoiceCodeId" />
    <property name="MakeId" />
    <property name="ModelId" />
    <property name="VariantId" />
    <property name="Position" />
    <property name="TireQuantity" />

    <property name="IsPolicyCreated" />
    <property name="PolicyId" />
    <property name="IsPolicyApproved" />
    <property name="PolicyCreatedDate" />
    <property name="Price" />

  </class>
</hibernate-mapping>