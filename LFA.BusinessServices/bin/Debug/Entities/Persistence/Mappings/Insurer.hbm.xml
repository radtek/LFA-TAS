<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="TAS.Services" namespace="TAS.Services.Entities">
  <class name="Insurer" table="Insurer" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="InsurerCode" />
    <property name="BookletCode" />
    <property name="InsurerShortName" />
    <property name="InsurerFullName" />
    <property name="Comments" />
    <property name="IsActive" />
    <property name="EntryDateTime" />
    <property name="EntryUser" />   

  </class>
</hibernate-mapping>