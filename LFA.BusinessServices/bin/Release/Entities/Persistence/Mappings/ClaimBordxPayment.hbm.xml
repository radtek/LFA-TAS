<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="TAS.Services" namespace="TAS.Services.Entities">
  <class name="ClaimBordxPayment" table="ClaimBordxPayment" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="ReceiptDate" />
    <property name="BordxAmount"  precision="18" scale="8"/>
    <property name="RefNo" />
    <property name="BalanceAmount"  precision="18" scale="8" />
    <property name="PaidAmount"  precision="18" scale="8" />
    <property name="ClaimBordxID" />
  </class>
</hibernate-mapping>