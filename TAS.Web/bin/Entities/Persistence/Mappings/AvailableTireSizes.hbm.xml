<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="TAS.Services" namespace="TAS.Services.Entities">
  <class name="AvailableTireSizes" table="AvailableTireSizes" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="Width" />
    <property name="CrossSection" />
    <property name="Diameter" />
    <property name="LoadSpeed" />

    <property name="TirePrice" />
    <property name="CurrencyRate" />
    <property name="CurrencyId" />
    <property name="CurrencyPeriodId" />
    <property name="OriginalTireDepth" />
  
  </class>
</hibernate-mapping>
