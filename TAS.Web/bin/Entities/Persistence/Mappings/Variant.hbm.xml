  <class name="Variant" table="Variant" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="ModelId" />
    <property name="CommodityTypeId" />
    <property name="VariantName" />
    <property name="FromModelYear" />
    <property name="ToModelYear" />
    <property name="EngineCapacityId" />
    <property name="CylinderCountId" />
    <property name="BodyCode" />
    <property name="IsActive" />
    <property name="EntryDateTime" />
    <property name="EntryUser" />
    <property name="GrossWeight" />
    <!--<property name="IsSports" />-->

    
    
  </class>
</hibernate-mapping>