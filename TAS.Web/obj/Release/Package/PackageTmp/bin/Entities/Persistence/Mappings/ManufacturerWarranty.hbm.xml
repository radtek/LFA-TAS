<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="TAS.Services" namespace="TAS.Services.Entities">
  <class name="ManufacturerWarranty" table="ManufacturerWarranty" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="MakeId" />
  
    <property name="ApplicableFrom" />

    <property name="WarrantyName" />
    <property name="WarrantyMonths" />
    <property name="WarrantyKm" />
    <property name="EntryDateTime" />
    <property name="EntryUser" />
    <property name="IsUnlimited" />

  </class>
</hibernate-mapping>