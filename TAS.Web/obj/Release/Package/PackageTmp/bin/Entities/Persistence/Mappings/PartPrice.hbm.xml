<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="TAS.Services" namespace="TAS.Services.Entities">
  <class name="PartPrice" table="PartPrice" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Guid">
      <generator class="guid" />
    </id>
    <property name="CountryId" />
    <property name="DealerId" />
    <property name="PartId" />

    
    <property name="CurrencyId" />
    <property name="CurrencyPeriodId" />
    <property name="Price" />
   
    <property name="ConversionRate" />

  </class>

</hibernate-mapping>